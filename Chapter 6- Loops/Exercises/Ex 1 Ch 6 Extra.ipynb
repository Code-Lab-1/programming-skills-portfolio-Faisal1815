        "prompt = \"\\nWhat topping would you like on your sandwich?\"\n",
        "prompt += \"\\nEnter 'stop' when you are finished: \"\n",
        "\n",
        "while True:\n",
        "    topping = input(prompt)\n",
        "    if topping != 'stop':\n",
        "        print(\"  I'll add \" + topping + \" to your sandwich.\")\n",
        "    else:\n",
        "        break"
      ],
